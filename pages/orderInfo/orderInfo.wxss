page {
  background: #f1f1f1;
}

.addrSelc-content {
  justify-content: space-between;
  padding: 4.5% 2% 4.5% 0;
  padding-left: 20rpx;
  background: #fff;
}

.addrSelc-content-text {
  font-size: 36rpx;
  /*color: #777777;*/
  overflow: auto;
  /*white-space: nowrap;*//*text-overflow: ellipsis;*/
}

.addrSelc-content-text1 {
  font-size: 30rpx;
  color: #777;
  overflow: auto;
  /*white-space: nowrap;*//*text-overflow: ellipsis;*/
}

.addrView {
  padding-top: 2%;
  padding-bottom: 1%;
  padding-left: 4%;
  background: #fff;
}

.addr-title {
  display: flex;
  /*justify-content: center;*/
  align-items: center;
  font-size: 32rpx;
  margin-bottom: 2%;
  /*color: #1392e3;*/
}

.addr-name {
  font-size: 34rpx;
}

.addr-phone {
  margin-left: 10%;
  font-size: 34rpx;
  color: #0a0a0a;
  font-weight: bolder;
}

.addr-display {
  height: 100%;
  line-height: 150%;
  margin-top: 1%;
  margin-right: 1%;
  justify-content: space-between;
}

.expressView {
  padding-top: 2%;
  padding-bottom: 1%;
  padding-left: 3%;
  background: #fff;
}

.express-title {
  font-size: 36rpx;
}

.express-info-itme {
  display: flex;
  justify-content: space-around;
  /*align-items: center;*/
  font-size: 32rpx;
  height: 120rpx;
  padding-bottom: 1%;
}

.express-info-itme1 {
  display: flex;
  justify-content: space-around;
  /*align-items: center;*/
  background-color: #f8f8f8;
  font-size: 32rpx;
  height: 120rpx;
  padding-bottom: 1%;
}

