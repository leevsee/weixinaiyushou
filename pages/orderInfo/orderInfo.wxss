.express-info-circle-wrap {
  display: flex;
  /*align-items: flex-end;*/
  margin-top: 60rpx;
  margin-left: 5%;
  padding-bottom: 0;
  border-left: solid 3rpx #fe7300;
  height: 100%;
}

.express-info-circle-wrap1 {
  display: flex;
  /*align-items: flex-end;*/
  margin-top: 60rpx;
  margin-left: 5%;
  padding-bottom: 0;
  border-left: solid 3rpx #fe7300;
  height: 0%;
}

.express-info-circle {
  /*display: block;*/
  position: relative;
  top: -12rpx;
  left: -12rpx;
  width: 24rpx;
  height: 24rpx;
  background-color: #fe7300;
  border-radius: 50%;
}

.express-info-date {
  padding-top: 1%;
  font-size: 26rpx;
  color: #777;
  /*font-weight: bolder;*/
}

.express-info-year {
  color: #fe7300;
}

.express-info-time {
  margin-top: 5%;
}

.express-info-text {
  /*position: relative;*/
  display: flex;
  /*align-items: flex-end;*/
  flex-direction: column-reverse;
  color: #777;
  width: 60%;
  height: auto;
  overflow: auto;
  /*white-space: pre-wrap;*//*text-overflow: clip;*/
}

.onSale-name {
  justify-content: space-around;
  padding-top: 2%;
  padding-bottom: 1%;
  padding-left: 20rpx;
  background: #fff;
}

.onSale-name-content {
  justify-content: space-between;
  align-items: stretch;
  height: 145rpx;
  width: 66%;
}

.onSale-name-text {
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 28rpx;
}

.onSale-name-price {
  font-size: 28rpx;
  font-weight: bold;
  color: #1392e3;
}

.onSale-name-buy {
  display: flex;
  width: 50%;
}

.onSale-name-num {
  width: 20%;
  font-size: 26rpx;
  /*height: 80%;*/
  text-align: center;
  background-color: #f2f2f2;
  line-height: 150%;
}

.onSale-name-but {
  width: 40rpx;
  height: 40rpx;
}

.onSale-postage {
  /*display: flex;*/
  justify-content: space-between;
  padding: 2% 4% 2% 2%;
  background: #fff;
}

.onSale-postage-price {
  font-size: 36rpx;
}

.total {
  justify-content: space-between;
  padding: 2% 4% 2% 0;
  padding-left: 20rpx;
  background: #fff;
}

.total-price {
  color: #1392e3;
  font-size: 36rpx;
  font-weight: bold;
}

.send-message {
  height: 150rpx;
  padding: 2% 4% 2% 0;
  padding-left: 20rpx;
  background: #fff;
}

.send-message-input {
  width: 96%;
  height: 150rpx;
  margin: 2% 4% 2% 2%;
  font-size: 34rpx;
  color: #666;
}

textarea:placeholder {
  color: red;
}

.orderInfo-wrap {
  background: #fff;
}

.orderInfo {
  display: flex;
  justify-content: space-between;
  padding: 3% 4%;
}

.thin-divide {
  margin-left: 4%;
  height: 2rpx;
  width: initial;
}

.onSale-num {
  display: flex;
  justify-content: space-between;
  margin-left: 2%;
  margin-top: 2%;
}

.onSale-num-price {
  width: 10%;
  height: 90%;
  text-align: center;
  background-color: antiquewhite;
  /*margin-right: 2%;
    margin-left: 2%;*/
}

.onSale-num-buy {
  display: flex;
  width: 50%;
}

.onSale-price {
  display: flex;
  flex-direction: column;
  margin-left: 2%;
  margin-top: 2%;
}

.order-btn {
  display: flex;
  justify-content: flex-end;
  background: #fff;
}

.order-payBtn {
  margin: 2%;
}

.order-cancalBtn {
  margin: 2%;
}

.cancalBtn {
  font-size: 32rpx;
  line-height: 2.55555556;
  /*border-radius: 15rpx;*/
  color: #000;
  /*border: 1rpx solid #777;*/
  background-color: #fff;
}

.cancalBtn::after {
  border: 1px solid #777;
}

.payBtn {
  font-size: 32rpx;
  line-height: 2.55555556;
  border-radius: 15rpx;
  overflow: hidden;
  color: #fff;
  border: 1rpx solid;
  background-color: #1392e3;
}

.pay-over-Btn {
  font-size: 32rpx;
  line-height: 2.55555556;
  border-radius: 15rpx;
  overflow: hidden;
  color: #fff;
  border: 1rpx solid;
  background-color: #9a9797;
}

.payBtn::after {
  border: 1px solid #1392e3;
}

.order-myBtn {
  border-radius: 5rpx;
  border-color: red;
}

.payBtn-hover {
  background-color: #40abf0;
}

.payBtn-over-hover {
  background-color: #9a9797;
}

.cancalBtn-hover {
  background-color: #ebe7e7;
}
